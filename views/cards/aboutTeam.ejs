<div>
    <h1 class="display-4 fw-bold text-center">About My Agency</h1>
    <div class="text-center mb-3">
        <% for(i=0; i < members.length; i++) { %>
            <p class="lead fw-bold">
                Newest Member Joined On <%= members[i].dateJoined.toLocaleString() %>
            </p>
        <% } %>
    </div>
    <div class="row mt-5 mb-5">
        <div class="col-md-4"
             data-mdb-toggle="animation"
             data-mdb-animation-start="onLoad"
             data-mdb-animation="fade-in-left"
             data-mdb-animation-delay="100">
            <div class="card hover-zoom m-3 p-2">
                <h4>Lead Developer</h4>
                <a href="/profile">
                    <img src="https://images.unsplash.com/photo-1498049860654-af1a5c566876?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80" class="img-fluid" alt="img">
                </a>

            </div>
        </div>
        <div class="col-md-4"
             data-mdb-toggle="animation"
             data-mdb-animation-start="onLoad"
             data-mdb-animation="fade-in"
             data-mdb-animation-delay="300">
            <div class="card hover-zoom m-3 p-2">
                <h4>Lead Designer</h4>
                <a href="/profile">
                    <img src="https://images.unsplash.com/photo-1583508915901-b5f84c1dcde1?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80" class="img-fluid" alt="img">
                </a>
            </div>
        </div>
        <div class="col-md-4"
             data-mdb-toggle="animation"
             data-mdb-animation-start="onLoad"
             data-mdb-animation="fade-in-right"
             data-mdb-animation-delay="500">
            <div class="card hover-zoom m-3 p-2">
                <h4>Scrum Manager</h4>
                <a href="/profile">
                    <img src="https://images.unsplash.com/photo-1542744095-0d53267d353e?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80" class="img-fluid" alt="img">
                </a>
            </div>
        </div>
    </div>
</div>